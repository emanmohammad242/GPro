<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.farhaapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FarhaApplication"
        android:usesCleartextTraffic="true">
        <activity android:name=".Acticites.AddCategory_Activity"></activity>
        <activity android:name=".Acticites.addProduct_Activity" />
        <activity android:name=".Acticites.MainActivitymmmm" />
        <activity android:name=".Acticites.UpDateProduct_Activity" />
        <activity android:name=".Acticites.Register_Activity" />
        <activity android:name=".Acticites.LogIn_Activity" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="true" />

        <activity android:name=".Acticites.MainActivityee" />
        <activity android:name=".Acticites.Products_Activity" />
        <activity android:name=".Acticites.Category_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>